{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["client/js/"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "readme": "README.md",
        "destination": "./docs/html/"
    }
}

